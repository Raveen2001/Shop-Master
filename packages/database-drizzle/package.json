{
  "name": "database-drizzle",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "pull": "dotenv -e .env.local -- drizzle-kit introspect:pg",
    "push": "dotenv -e .env.local -- drizzle-kit push:pg",
    "generate": "dotenv -e .env.local -- drizzle-kit generate:pg",
    "drop": "dotenv -e .env.local -- drizzle-kit drop",
    "up": "dotenv -e .env.local -- drizzle-kit up:pg",
    "check": "dotenv -e .env.local -- drizzle-kit check:pg",
    "studio": "dotenv -e .env.local -- drizzle-kit studio --port 9010 --verbose",
    "migrate": "dotenv -e .env.local -- ts-node migrate.ts",
    "build": "tsc",
    "dev": "tsc --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "drizzle-orm": "^0.28.5",
    "postgres": "^3.3.5",
    "ts-transformer-keys": "^0.4.4"
  },
  "devDependencies": {
    "@types/node": "20.5.7",
    "drizzle-kit": "^0.19.13",
    "pg": "^8.11.3",
    "typescript": "^5.6.3",
    "tsconfig": "*"
  }
}
