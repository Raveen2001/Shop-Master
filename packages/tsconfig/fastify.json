{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Fastify",

  "include": ["**/*.ts"],

  "extends": "./base.json",
  "compilerOptions": {
    "outDir": "./dist",
    "baseUrl": ".",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "ES2022"
  },


  "exclude": ["node_modules"]
}
